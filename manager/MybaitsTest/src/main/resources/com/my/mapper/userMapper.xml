<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.my.mapper.userMapper">
  <resultMap id="BaseResultMap" type="com.my.pojo.user">
    <result column="Id" jdbcType="INTEGER" property="id" />
    <result column="CreateTime" jdbcType="TIMESTAMP" property="createtime" />
    <result column="Name" jdbcType="VARCHAR" property="name" />
    <result column="Age" jdbcType="INTEGER" property="age" />
    <result column="UserStatus" jdbcType="INTEGER" property="userStatus" typeHandler="com.my.MyEumeTypeHandler" />
  </resultMap>
  <select id="selectAll" resultMap="BaseResultMap">
    select  * from my_user
  </select>

  <select id="selectById" resultMap="BaseResultMap">
    select  * from my_user where id=#{0}
  </select>

  <insert id="insert" parameterType="com.my.pojo.user">
    insert into my_user (Id, CreateTime, Name, 
      Age)
    values (#{id,jdbcType=INTEGER}, #{createtime,jdbcType=TIMESTAMP}, #{name,jdbcType=VARCHAR}, 
      #{age,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="com.my.pojo.user">
    insert into my_user
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
        Id,
      </if>
      <if test="createtime != null">
        CreateTime,
      </if>
      <if test="name != null">
        Name,
      </if>
      <if test="age != null">
        Age,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id,jdbcType=INTEGER},
      </if>
      <if test="createtime != null">
        #{createtime,jdbcType=TIMESTAMP},
      </if>
      <if test="name != null">
        #{name,jdbcType=VARCHAR},
      </if>
      <if test="age != null">
        #{age,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
</mapper>